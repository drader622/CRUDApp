   <body class="">
   <nav class="nav flex">
         <div id="listToggle">
            <input type="checkbox" />
            <span class="line"></span>
            <span class="line"></span>
            <span class="line"></span>
            <ul class="navButtons flex">
               <a href="/account" id="accountInfo" class="accountInfoMain darkBackground"><i class="icon fa-solid fa-user darkLettering"></i></a>
               <a href="#" id="moduleInfo" class="moduleInfoMain darkBackground"><i class="icon fa-solid fa-industry darkLettering"></i></a>
               <a href="/workOrders" class="workOrdersMain darkBackground"><i id="workOrdersInfo" class="fa-solid fa-clipboard-list darkLettering"></i></a>
               <a href="#" class="pmInfoMain darkBackground"><i class="fa-solid fa-calendar-day darkLettering"></i></a>
               <a href="#" class="storeRoomInfoMain darkBackground"><i class="fa-solid fa-warehouse darkLettering"></i></a>
               <a href="#" class="messagingMain darkBackground"><i class="fa-solid fa-comment darkLettering"></i></a>
               <a href="#" class="coworkerInfoMain darkBackground"><i class="fa-solid fa-people-group darkLettering"></i></a>
            </ul>
         </div>
      </nav>

      <section class="mainContainer">
         <section class="employee flex">
            <img class="logo" src="/images/default-monochrome.svg" alt="">
            </img>            
            <section class="flex">
               <section class="container flex">
                  <span id="name">Danielle Rader</span>
                  <span id="title">Industrial Electrician</span>
                  <span>Engineering</span>
               </section>
               <span class="userPic"
               ><img class="headshot" src="/images/headshot.png" alt=""
               /></span>
               <a href="/user" class="logout">Logout</a>
            </section>
            <section>
               <p>Dark Mode</p>
               <input type="checkbox" id="switch" class="checkbox" />                 
               <label for="switch" class="toggle">
               </label>
            </section>
         </section>
         <section class="main">
            <section class="accountInfoMain flex">
               <h1>User Profile</h1>
               <section class="flex">
                  <h2>Name</h2>
                  <input type="text" value="Danielle Rader" disabled />
               </section>
               <section class="job flex">
                  <section>
                     <h2>Job Title</h2>
                     <input
                        type="text"
                        value="Industrial Electrician"
                        disabled
                     />
                  </section>
                  <section class="payInput">
                     <h2>Pay</h2>
                     <input type="text" value="$XX.XX" disabled />
                  </section>
               </section>
               <section class="crew flex">
                  <section>
                     <h2>Supervisor</h2>
                     <input type="text" value="Ted Bonham" disabled />
                  </section>
                  <section class="crewInput">
                     <h2>Crew</h2>
                     <input type="text" value="C" disabled />
                  </section>
               </section>
               <section>
                  <h2>Address</h2>
                  <input type="text" value="605 N Ash St" disabled />
                  <input type="text" value="" disabled />
               </section>
               <section>
                  <h2>City</h2>
                  <input type="text" value="Deshler" disabled />
               </section>
               <section>
                  <h2>State</h2>
                  <input type="text" value="OH" disabled />
               </section>
               <section>
                  <h2>Zip</h2>
                  <input type="text" value="43516" disabled />
               </section>
            </section>
         </section>
      </section>
